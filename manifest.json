{
    "manifest_version": 3,
    "name": "Zendesk Alerts",
    "version": "0.1",
    "description": "Alert to new tickets for the clients you need to track of!",
    "permissions": [
        "notifications",
        "scripting",
        "activeTab",
        "storage"
    ],
    "icons": {
        "16": "images/Zendesk.png",
        "32": "images/Zendesk.png",
        "48": "images/Zendesk.png",
        "128": "images/Zendesk.png"
      },
    "action": {
        "default_popup": "app/popup.html"
    },
    "background": {
        "service_worker": "scripts/background.js",
        "type": "module"
    }
}